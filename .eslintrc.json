{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "default-param-last": 0,
        "react/destructuring-assignment": 0,
        "react/state-in-constructor": 0,
        "arrow-body-style": 0,
        "no-unneeded-ternary": 0,
        "class-methods-use-this": 0,
        "react/forbid-prop-types": 0,
        "react/jsx-one-expression-per-line": 0
    }
}
